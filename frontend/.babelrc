{
  "sourceMaps": "both",
  "presets": [
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-es2015-modules-commonjs",
    "add-module-exports",
    "transform-exponentiation-operator",
    "syntax-async-functions",
    ["transform-object-rest-spread", {
      "useBuiltIns": true
    }],
    ["fast-async", {
      "runtimePatten": "directive",
      "compiler": {
        "promises": false,
        "es7": true,
        "lazyThenables": true
      }
    }]
  ],
  "env": {
    "test": {
      "plugins": [
        "transform-async-to-generator", [
          "transform-runtime", {
            "polyfill": false,
            "regenerator": false
          }
        ]
      ]
    }
  }
}
